rm.js","original":{"line":63,"column":47}},{"name":"target","generated":{"line":69,"column":36},"source":"../src/AddQuoteForm.js","original":{"line":63,"column":48}},{"generated":{"line":69,"column":42},"source":"../src/AddQuoteForm.js","original":{"line":63,"column":46}},{"name":"value","generated":{"line":69,"column":43},"source":"../src/AddQuoteForm.js","original":{"line":63,"column":55}},{"generated":{"line":69,"column":48},"source":"../src/AddQuoteForm.js","original":{"line":63,"column":45}},{"generated":{"line":70,"column":0},"source":"../src/AddQuoteForm.js","original":{"line":60,"column":12}},{"generated":{"line":70,"column":4},"source":"../src/AddQuoteForm.js","original":{"line":45,"column":8}},{"generated":{"line":70,"column":19},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":12}},{"generated":{"line":71,"column":0},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":20}},{"generated":{"line":71,"column":4},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":20}},{"generated":{"line":71,"column":9},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":25}},{"generated":{"line":71,"column":11},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":27}},{"name":"backgroundColor","generated":{"line":72,"column":0},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":28}},{"name":"backgroundColor","generated":{"line":72,"column":6},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":28}},{"generated":{"line":72,"column":21},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":43}},{"generated":{"line":72,"column":23},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":45}},{"generated":{"line":73,"column":0},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":27}},{"generated":{"line":74,"column":0},"source":"../src/AddQuoteForm.js","original":{"line":74,"column":12}},{"generated":{"line":74,"column":16},"source":"../src/AddQuoteForm.js","original":{"line":45,"column":8}},{"generated":{"line":74,"column":17},"source":"../src/AddQuoteForm.js","original":{"line":35,"column":4}},{"generated":{"line":74,"column":18},"source":"../src/AddQuoteForm.js","original":{"line":34,"column":2}},{"generated":{"line":75,"column":0},"source":"../src/AddQuoteForm.js","original":{"line":80,"column":1}},{"generated":{"line":75,"column":1},"source":"../src/AddQuoteForm.js","original":{"line":6,"column":0}},{"name":"CreateQuote","generated":{"line":77,"column":15},"source":"../src/AddQuoteForm.js","original":{"line":82,"column":15}}],"sources":{"../src/AddQuoteForm.js":"import { contentType } from 'mime-types';\nimport React, { useEffect, useState } from 'react';\nimport { post } from 'request';\n\n\nconst CreateQuote = () =>{\n\n  //creating a react hook to store the quote body\n  const [quote,setNewQuote] = useState('')\n  const [author, setQuoteAuthor] = useState('')\n\n  const handleSubmit = async (e) => { //creating a function to be executed when the submit button is pressed\n\n    e.preventDefault() //going to stop the page from refresehing after submitting\n\n      const newQuote = {quote,author}  //created a variable to store the new objest values set by the user\n\n        try {\n          const response = await fetch(`http://localhost:3000/motivation/add` ,{ //adding the route to be executed by the server\n          method: 'POST',  //the type of request\n          headers: {'Content-Type': 'application/json'},\n          body: JSON.stringify(newQuote) //turning the new object into a string to pass through the server \n          }).then((res)=>{\n            console.log(res) //logging success if the request went through with updating the database\n            alert(`Your quote has been added.\\n \n            Quote:\"${JSON.stringify(quote)}\"\n            Author: ${JSON.stringify(author)}`)// will alert the user when the quote was added and display a copy of the quote & author\n          })\n        }catch(err){\n          console.error(err) //logging error if the request was unsuccessful\n          alert('Unable to add your quote')\n        }\n  }\n  return(\n    <div className= \"CreateQuote\" >\n      {/* Header for the form */}\n      <h2> Add your quote below: </h2>\n          {/* initializing the form */}\n\n          {/* Displaying the quote for the user to see how it will show display in the end\n          <h4>How your quote will be saved:</h4>\n          <p> \"{quote}\"</p> \n          <p>-{author}</p> */}\n\n        <form onSubmit={handleSubmit}>\n            \n            {/* Adding field to input quote context */}\n            <label> Add quote content:</label>\n            <textarea\n                required\n                value = {quote}\n                // changing the state of quote to update when user inputs text\n                // using the e.target.value to target the even object which is the text area and the value within\n                onChange = {(e)=> setNewQuote (e.target.value)}  \n                \n            />\n\n            {/* adding field to add quote author */}\n            <label> Quote author: </label>\n            <input \n            required \n            value = {author}\n            onChange ={ (e) => setQuoteAuthor(e.target.value)}\n            />\n\n             {/* stretch goal--- add the current username to pop up below\n            <label>User Name: </label>\n            <select>\n              \n              <option value='anonymous'>Anonymous</option>\n            </select> */}\n\n            {/* adding a submit button to save the changes */}\n            <button style={{backgroundColor: \"lightblue\"}}> Submit </button>\n            \n\n        </form>\n    </div>\n  )\n}\n\nexport default CreateQuote\n"},"lineCount":null}},"error":null,"hash":"85a1ef1eb9ab0826d467ae7b3bc48ada","cacheData":{"env":{}}}